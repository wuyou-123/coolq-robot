<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.wuyou.mapper.ClearMapper">
	<delete id="clearMessage">
		delete from message where groupId=#{groupId}
	</delete>
	<delete id="clearBanMessage">
		delete from banMessage where groupId=#{groupId}
	</delete>
	<delete id="clearBlackUser">
		delete from blackUser where groupId=#{groupId}
	</delete>
	<delete id="clearManager">
		delete from manager where groupId=#{groupId}
	</delete>
	<delete id="clearStat">
		delete from stat where groupId=#{groupId}
	</delete>
</mapper>